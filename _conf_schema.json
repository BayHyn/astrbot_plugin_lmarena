{
    "prefix":{
        "description": "启用触发前缀",
        "hint": "启用后需要前缀或者@bot来触发命令",
        "type": "bool",
        "default": "!img"
    },
    "base_url":{
        "description": "生图服务器URL",
        "type": "string",
        "default": "http://127.0.0.1:5102"
    },
    "model":{
        "description": "生图模型",
        "hint": "建议默认nano-banana",
        "type": "string",
        "default":"nano-banana"
    },
    "save_image":{
        "description": "保存生成的图片",
        "hint": "保存目录为插件数据目录",
        "type": "bool",
        "default":false
    },
    "retries":{
        "description":"生图失败重试次数",
        "hint": "最后一次重试仍失败时，将返回错误",
        "type": "int",
        "default": 2
    },
    "prompt": {
        "description": "生图触发词与提示词",
        "hint": "影响手办化的效果，请前往data/plugins/astrbot_plugin_lmarena/prompt.py更改, 格式为 触发词:提示词",
        "type": "string",
        "default": "请前往data/plugins/astrbot_plugin_lmarena/prompt.py更改"
    }
}